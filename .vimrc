" set t_Co=256

" for hopping words
nnoremap [C <C-Right>
inoremap [C <C-Right>

" default Terminal.app opt+left
nnoremap f <C-Right>
inoremap f <C-Right>

nnoremap [D <C-Left>
inoremap [D <C-Left>

" default Terminal.app opt+right
nnoremap b <C-Left>
inoremap b <C-Left>

set term=xterm-256color
colorscheme TMEighties
set t_ut=
set mouse=a

