--langdef=objcpp
--langmap=objcpp:.mm.m.h
--regex-objcpp=/\@interface[[:space:]]+([[:alnum:]_]+)[[:space:]]*:/\1/i,interface/
--regex-objcpp=/\@interface[[:space:]]+([[:alnum:]_]+)[[:space:]]*\(([[:alnum:]_]*)\)/\1(\2)/x,extension/
--regex-objcpp=/\@implementation[[:space:]]+([[:alnum:]_]+)[[:space:]]*$/\1/I,implementation/
--regex-objcpp=/\@implementation[[:space:]]+([[:alnum:]_]+)[[:space:]]*\(([[:alnum:]_]*)\)/\1(\2)/I,implementation/
--regex-objcpp=/\@protocol[[:space:]]+([[:alnum:]_]+)/\1/P,protocol/
--regex-objcpp=/\@property[[:space:]]*\([[:alnum:],=[:space:]]+\)*[[:space:]]+([^;]*)[[:space:]]+([[:alnum:]_]+)[[:space:]]*;$/\2/p,property/
--regex-objcpp=/\@property[[:space:]]*\([[:alnum:],=[:space:]]+\)*[[:space:]]+[[:alnum:]]+[[:space:]]*<[[:alnum:]]+>[[:space:]]+([[:alnum:]_]+)/\1/p,property/
--regex-objcpp=/^[[:space:]]*([-+])[[:space:]]*\([[:alpha:]_][^)]*\)[[:space:]]*(([[:alpha:]_][^:;{]+:?)(\([^)]+\)[[:alnum:]_]+[[:space:]\n]*)?){1,}/\1\3\6\9/M,method definition/
--regex-objcpp=/^[^#@[:space:]][^=]*[[:space:]]([[:alpha:]_][[:alnum:]_]*)[[:space:]]*=/\1/c,constant/
--regex-objcpp=/^[[:space:]]*typedef[[:space:]][^;]+[[:space:]]([[:alpha:]_][[:alnum:]]*)[[:space:]]*;/\1/t,typedef/
--regex-objcpp=/[[:space:]]NS_ENUM\([[:alnum:]]+[[:space:]]*,[[:space:]]*([[:alnum:]]+)\)/\1/e,enumeration/
--regex-objcpp=/^#pragma[[:space:]]+mark[[:space:]]+-?[[:space:]]+([[:alnum:][:space:]]+)/\1/g,pragma/
