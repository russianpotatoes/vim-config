  go_left = <Primary>h
  go_next = None
  go_prev = None
  go_right = <Primary>l
  go_up = <Primary>k
  resize_down = <Alt>j
  resize_left = <Alt>h
  resize_right = <Alt>l
  resize_up = <Alt>k
  rotate_ccw = <Alt>u
  rotate_cw = <Alt>i
  split_horiz = <Primary>minus
  split_vert = <Primary>backslash
